<h1>Sign In</h1>
<p>No account yet? <%= link_to "Sign Up Here!", new_user_path %>

<%= form_with(url: session_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email, autofocus: true %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign In"%>
<% end %>
